    },
    "diagnostic.highlightOffset": 10,
    "diagnostic.enable":true,
    "diagnostic.showUnused": true,
    "diagnostic.errorSign": "✖",
    "diagnostic.warningSign": "➤ ",
    "diagnostic.hintSign": "›",
    "diagnostic.infoSign": "›",

    "diagnostic.level": "hint",
    "diagnostic.messageDelay": 10,
    "diagnostic.displayByAle":false,
    "diagnostic.virtualText":true,
    "diagnostic.virtualTextLines": 10,
    "diagnostic.virtualTextPrefix": " ½ ",
    "diagnostic.virtualTextCurrentLineOnly": false,
    "diagnostic.showDeprecated": true,
    "diagnostic.autoRefresh": true,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.maxWindowWidth": 60,
    "diagnostic.maxWindowHeight": 30,
    "diagnostic.messageTarget":"float",
    "diagnostic.enableMessage":"jump",
    "diagnostic.enableHighlightLineNumber":true,
    "diagnostic.refreshOnInsertMode":true,
    "diagnostic.format":"%message [%source%code] [%severity]",

    "translator.enableHover": false,
    "translator.engines": [
        "bing",
        "youdao"
    ],
    "translator.window.enableBorder": true,

    "explorer.width": 26,
    "explorer.position":"floating",

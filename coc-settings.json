{
    "list.indicator": ">",
    "list.nextKeymap": "<C-j>",
    "list.previousKeymap": "<C-k>",
    "coc.preferences.floatActions": true,
    "coc.preferences.enableMessageDialog": true,
    "coc.preferences.colorSupport": true,
    "coc.preferences.hoverTarget":"float",
    "coc.preferences.enableFloatHighlight": true,
    "coc.preferences.formatOnType": true,
    "coc.preferences.snippets.enable": true,
    "coc.preferences.promptInput": true,
    "dialog.confirmKey": "<cr>",
    "suggest.detailField":"abbr",
    "suggest.echodocSupport": true,
    "suggest.noselect":true,
    "suggest.enablePreselect": false,
    "suggest.floatEnable":true,
    "suggest.enablePreview": true,
    "suggest.autoTrigger":"always",
    "suggest.triggerCompletionWait": 100,
    "suggest.completionItemKindLabels": {
      "keyword": "key",
      "variable": "var",
      "operator": "+",
      "constructor": "cons",
      "function": "func",
      "reference": "ref",
      "constant": "K",
      "method": "func",
      "struct": "struct",
      "class": "class",
      "interface": "interface",
      "text": "text",
      "enum": "enum",
      "enumMember": "enumM",
      "module": "modu",
      "color": "color",
      "property": "property",
      "field": "field",
      "unit": "unit",
      "event": "event",
      "file": "file",
      "folder": "folder",
      "snippet": "snippet",
      "typeParameter": "typeParam",
      "default": ""
    },
    "snippets.extends": {
        "cpp" : ["cpp", "c"],
        "c" : ["c"]
    },
    "diagnostic.enable":true,
    "diagnostic.showUnused": true,
    "diagnostic.errorSign": "!>",
    "diagnostic.warningSign": ">>",
    "diagnostic.hintSign": "H>",
    "diagnostic.infoSign": "I>",
    "diagnostic.enableSign": true,
    "diagnostic.level": "hint",
    "diagnostic.messageDelay": 100,
    "diagnostic.displayByAle":false,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualText":true,
    "diagnostic.virtualTextLines": 10,
    "diagnostic.virtualTextPrefix": "> ",
    "diagnostic.virtualTextCurrentLineOnly": true,
    "diagnostic.format":"%message [%source%code] [%severity]",

    "translator.enableHover": false,
    "translator.engines": [
        "bing",
        "youdao"
    ],
    "translator.window.enableBorder": true

}

{
    "list.indicator": ">",
    "highlight.colors.enable":false,
    "list.nextKeymap": "<C-j>",
    "list.previousKeymap": "<C-k>",
    "coc.preferences.floatActions": true,
    "coc.preferences.enableMessageDialog": true,
    "coc.preferences.colorSupport": true,
    "coc.preferences.hoverTarget":"float",
    "coc.preferences.enableFloatHighlight": true,
    "coc.preferences.formatOnType": true,
    "coc.preferences.snippets.enable": true,
    "coc.preferences.promptInput": true,
    "suggest.detailField":"abbr",
    "dialog.confirmKey": "<cr>",
    "suggest.echodocSupport": true,
    "suggest.acceptSuggestionOnCommitCharacter":true,
    "suggest.preferCompleteThanJumpPlaceholder":true,
    "suggest.localityBonus": true,
    "suggest.noselect":true,
    "suggest.enablePreselect":true,
    "suggest.floatEnable":true,
    "suggest.enablePreview": true,
    "suggest.autoTrigger":"always",
	  "suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\ue79b",
		"value": "\uf89f",
		"operator": "\u03a8",
		"constructor": "\uf0ad",
		"function": "\u0192",
		"reference": "\ufa46",
		"constant": "\uf8fe",
		"method": "\uf09a",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\uf417",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\uf40d",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf9be",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\ue60b",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},
    "suggest.triggerCompletionWait": 100,
    "snippets.extends": {
        "cpp" : ["cpp", "c"],
        "c" : ["c"]
    },
    "diagnostic.highlightOffset": 10,
    "diagnostic.enable":true,
    "diagnostic.showUnused": true,
    "diagnostic.errorSign": "⛌",
    "diagnostic.warningSign": "⟁ ",
    "diagnostic.hintSign": "›",
    "diagnostic.infoSign": "›",
    "diagnostic.enableSign": true,
    "diagnostic.level": "hint",
    "diagnostic.messageDelay": 100,
    "diagnostic.displayByAle":false,
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualText":true,
    "diagnostic.virtualTextLines": 10,
    "diagnostic.virtualTextPrefix": " ⮩ ",
    "diagnostic.virtualTextCurrentLineOnly": false,
    "diagnostic.showDeprecated": true,
    "diagnostic.enableHighlightLineNumber":true,
    "diagnostic.refreshOnInsertMode":true,
    "diagnostic.format":"%message [%source%code] [%severity]",

    "translator.enableHover": false,
    "translator.engines": [
        "bing",
        "youdao"
    ],
    "translator.window.enableBorder": true,

    "explorer.width": 30,
    "explorer.filename.colored.enable": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.floating.position": "center-top",
    "explorer.keyMappings.global": {
      "u" : ["wait", "gotoParent"]
    },
    "rainbow-fart.enabled": true,
    "rainbow-fart.locale": ["zh"],
    "rainbow-fart.ffplay": "ffplay",
    "rainbow-fart.disable-voice-packages": []
}
